import pandas as pd
import numpy as np

d = pd.read_csv('data.csv')
cols = list(d.columns)
print(cols)
cols.remove('Banks')
cols.remove('Solvent')
print(cols)

from sklearn.ensemble import RandomForestClassifier
from sklearn.cross_validation import cross_val_score, StratifiedShuffleSplit

X = d[cols]
y = d['Solvent']

cv = StratifiedShuffleSplit(y,
        n_iter=5,
        test_size=0.2,
        random_state=0,)
classifier = RandomForestClassifier(
            n_estimators=500,
            max_depth=None,
            min_samples_split=1, 
            random_state=0)
score = cross_val_score(classifier, X, y, cv=cv, scoring='roc_auc')
print("AUC:  {:.2%} (+/- {:.2%})".format(score.mean(), 
            score.std()))
